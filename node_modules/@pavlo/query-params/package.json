{
  "name": "@pavlo/query-params",
  "version": "0.1.0-beta.0",
  "description": "Hooks for typed URL query params with batch updates (React + react-router-dom).",
  "author": "Pavlo Kuzina",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19",
    "react-router-dom": "^6.4 || ^7"
  },
  "devDependencies": {
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/<your-username>/<repo>.git"
  },
  "bugs": {
    "url": "https://github.com/<your-username>/<repo>/issues"
  },
  "homepage": "https://github.com/<your-username>/<repo>#readme",
  "keywords": [
    "react",
    "hooks",
    "url",
    "query",
    "params",
    "typescript"
  ]
}
